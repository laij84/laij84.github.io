{
    "extends": ["airbnb", "prettier"],
    "plugins": [
        "prettier",
        "import",
        "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
        "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "globals": {
        "graphql": false
      },
    "rules": {
        "prettier/prettier": "error",
        "import/no-extraneous-dependencies": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4]
    }
}